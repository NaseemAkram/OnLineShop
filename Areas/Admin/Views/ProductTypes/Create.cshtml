@model OnLineShop.Models.ProductTypes
@{
    ViewData["Title"] = "Create";
}






<h1 class="text-info">Create Product Types</h1>
<br />


<div>
    <form method="post" action="Create">
        <div class="p-4 rounded-circle">
            <div asp-validation-summary="ModelOnly" class="text-danger">

            </div>
            <div class="form-group row">
                <div class="col-2">
                    <label asp-for="ProductType"></label>
                </div>
                <div>
                    <input asp-for="ProductType" class="form-control" />
                </div>
                <span asp-validation-for="ProductType" class="text-danger"></span>
            </div>
          
            <br />
            <div class="form-group">
                <input type="submit" class="btn btn-primary" value="save" />
                <a class="btn btn-dark" asp-action="Index">Back</a>
            </div>
        </div>
    </form>
</div>


